@using System.Web.Optimization
@using PassCache
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>passcache</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Styles.Render(BundlesConfig.StylesLocation)
</head>

<body>
    <div class="wrapin">
        <div class="wrapout">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-xs-12  col-sm-10 col-sm-offset-1 col-md-offset-2 col-md-8">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="git">
        <a href="https://github.com/vsliouniaev/pass-cache">
            <img alt="github" src="~/Static/GitHub-Mark-32px.png" height="25" width="25"/></a>
    </div>
    @RenderSection("scripts", required: false)
    @Scripts.Render(BundlesConfig.ScriptsLocation)
</body>
</html>

